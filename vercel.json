{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": null,
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": ".",
  "public": true,
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/assets/product-docs/(.*).mp4",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/mp4"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/product-docs/(.*).webm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/webm"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        }
      ]
    },
    {
      "source": "/assets/product-docs/(.*).mov",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/quicktime"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        }
      ]
    },
    {
      "source": "/assets/product-docs/(.*).ogg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/ogg"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        }
      ]
    }
  ]
}

